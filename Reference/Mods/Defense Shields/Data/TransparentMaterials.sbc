    <TransparentMaterial>
	<Id>
        <TypeId>TransparentMaterialDefinition</TypeId> <!-- IGNORE -->
		<SubtypeId>DS_HUD</SubtypeId> <!-- This is the name of the material as it is named in the 3D modelling software (transparent materials are no longer double sided mesh, require duplicate normals reversed mesh for other side) -->
	</Id>      
	<AlphaMistingEnable>false</AlphaMistingEnable> <!-- IGNORE -->
	<AlphaSaturation>1</AlphaSaturation> <!-- IGNORE -->
	<CanBeAffectedByOtherLights>false</CanBeAffectedByOtherLights> <!-- Whether or not other lights will cast light onto this texture; Suggest 'true' for windows -->
	<IgnoreDepth>false</IgnoreDepth> <!-- IGNORE -->
	<SoftParticleDistanceScale>1</SoftParticleDistanceScale> <!-- IGNORE -->
	<Texture>Textures\Models\Cubes\DS_HUD.dds</Texture> <!-- Your transparent material texture file aka DDS goes here -->
	<!--<GlossTexture>Textures\Models\Cubes\SolarPanel_ng.dds</GlossTexture>--> <!-- NEW -->
	<UseAtlas>false</UseAtlas> <!-- IGNORE -->
	<NeedSort>true</NeedSort> <!-- IGNORE -->
	<UVOffset>
		<X>0</X> <!-- IGNORE -->
		<Y>0</Y> <!-- IGNORE -->
	</UVOffset>
	<UVSize>
		<X>1</X> <!-- IGNORE -->
		<Y>1</Y> <!-- IGNORE -->
	</UVSize>
	<Color>
		<X>1</X> <!-- X,Y,Z: Acts sort of like Gamma for the texture; 0=dark 1=normal -->
		<Y>1</Y>
		<Z>1</Z>
		<W>1</W> <!-- Use this to adjust transparency; 0=transparent 1=opaque -->
	</Color>
	<ColorAdd>
		<X>0.0</X> <!-- X,Y,Z: Adds artificial color to texture; Suggested to leave this at 0 -->
		<Y>0.0</Y>
		<Z>0.0</Z>
		<W>.1</W> <!-- Keep this value at 0.1; Use <color><W> to fine tune transparency -->
	</ColorAdd>
	<ShadowMultiplier>
		<X>1</X>  <!-- controls "emissivity", suggest 0 for windows, 1 for force fields -->
		<Y>1</Y>
		<Z>1</Z>
		<W>1</W>  <!-- controls "darkening" effect of glass in shadow, 1 for no change, higher for darker tinted glass in shadow -->
	</ShadowMultiplier>
	<LightMultiplier> <!-- I don't notice any difference between 0 and 50 -->
		<X>1</X>
		<Y>1</Y>
		<Z>1</Z>
		<W>1</W>
	</LightMultiplier>
	<Reflectivity>0</Reflectivity> <!-- Controls reflectivity; Suggest very low numbers, for example 0.05 -->
	<Fresnel>0</Fresnel> <!-- Suggest 1 to 1.5 for windows; Hard to explain, sort of like reflectivity angle; Suggest 0 for no effect on 'force fields' -->
	<Gloss>0</Gloss> <!-- How strong the gloss material is -->
